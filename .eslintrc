{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "object-curly-spacing": ["warn", "never"],
        "eol-last": ["warn", "always"]
    },
    "ignorePatterns": [],
    "globals": {
        "__DEV__": true
    }
}
